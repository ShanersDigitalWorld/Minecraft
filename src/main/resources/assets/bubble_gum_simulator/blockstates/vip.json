{
  "multipart": [
    {
      "apply": {
        "model": "bubble_gum_simulator:block/vip_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vip_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vip_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vip_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vip_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vip_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vip_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vip_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vip_noside",
        "y": 270
      }
    }
  ]
}