{
  "multipart": [
    {
      "apply": {
        "model": "bubble_gum_simulator:block/vipwall_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vipwall_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vipwall_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vipwall_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vipwall_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vipwall_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vipwall_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vipwall_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "bubble_gum_simulator:block/vipwall_noside",
        "y": 270
      }
    }
  ]
}